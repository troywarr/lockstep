{
  "framework": "mocha",
  "before_tests": "coffee -c test/*.coffee",
  "src_files": [
    "src/index.coffee",
    "test/*.coffee"
  ],
  "serve_files": [
    "node_modules/chai/chai.js",
    "node_modules/sinon/pkg/sinon.js",
    "dist/lockstep.js",
    "test/*.js"
  ],
  "after_tests": "rm test/*.js",
  "launch_in_dev": [
    "Chrome",
    "Firefox",
    "Safari"
  ]
}
